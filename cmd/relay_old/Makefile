# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=release
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),release)
  relay_config = release
endif
ifeq ($(config),debug)
  relay_config = debug
endif

PROJECTS := relay

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

relay:
ifneq (,$(relay_config))
	@echo "==== Building relay ($(relay_config)) ===="
	@${MAKE} --no-print-directory -C . -f relay.make config=$(relay_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f relay.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  release"
	@echo "  debug"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   relay"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"